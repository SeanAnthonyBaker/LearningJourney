{
    "_version": "1.49.0",
    "sap.app": {
        "id": "project1",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:lrop",
            "version": "1.8.6",
            "toolsId": "2c2b14bd-b354-4c4b-801a-69def633152f"
        },
        "dataSources": {
            "mainService": {
                "uri": "/service/TrackLearning/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.110.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "project1.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "config": {
                "flexibleColumnLayout": {
                    "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
                    "defaultThreeColumnLayoutType": "ThreeColumnsEndExpanded"
                },
                "routerClass": "sap.f.routing.Router"
            },
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "epicList",
                    "target": [
                        "epicList"
                    ]
                },
                {
                    "pattern": "epic({key}):?query:",
                    "name": "epicObjectPage",
                    "target": [
                        "epicList",
                        "epicObjectPage"
                    ]
                },
                {
                    "pattern": "epic({key})/stories({key2}):?query:",
                    "name": "storiesObjectPage_stories",
                    "target": [
                        "epicList",
                        "epicObjectPage",
                        "storiesObjectPage_stories"
                    ]
                }
            ],
            "targets": {
                "epicList": {
                    "type": "Component",
                    "id": "epicList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "epic",
                            "variantManagement": "None",
                            "navigation": {
                                "epic": {
                                    "detail": {
                                        "route": "epicObjectPage"
                                    }
                                }
                            },
                            "hideFilterBar": false,
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "enableExport": false,
                                        "personalization": {
                                            "column": false,
                                            "filter": false,
                                            "sort": false
                                        },
                                        "quickVariantSelection": {
                                            "hideTableTitle": false,
                                            "showCounts": false
                                        },
                                        "condensedTableLayout": true,
                                        "selectionMode": "None"
                                    },
                                    "columns": {
                                        "DataField::epic_nm": {
                                            "width": "7cm"
                                        }
                                    }
                                }
                            },
                            "initialLoad": "Enabled"
                        }
                    },
                    "controlAggregation": "beginColumnPages",
                    "contextPattern": ""
                },
                "epicObjectPage": {
                    "type": "Component",
                    "id": "epicObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "epic",
                            "navigation": {
                                "stories": {
                                    "detail": {
                                        "route": "storiesObjectPage_stories"
                                    }
                                }
                            },
                            "content": {
                                "header": {
                                    "visible": true,
                                    "anchorBarVisible": false
                                }
                            },
                            "showRelatedApps": false,
                            "variantManagement": "None",
                            "controlConfiguration": {
                                "stories/@com.sap.vocabularies.UI.v1.LineItem#ListofStories": {
                                    "tableSettings": {
                                        "enableExport": false,
                                        "enableFullScreen": false,
                                        "enablePaste": false,
                                        "personalization": {
                                            "column": false,
                                            "filter": false,
                                            "sort": false
                                        },
                                        "quickVariantSelection": {
                                            "hideTableTitle": false,
                                            "showCounts": false
                                        },
                                        "selectAll": false,
                                        "selectionMode": "None",
                                        "condensedTableLayout": true
                                    },
                                    "columns": {
                                        "DataField::story_nm": {
                                            "width": "7cm"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "controlAggregation": "midColumnPages",
                    "contextPattern": "/epic({key})"
                },
                "storiesObjectPage_stories": {
                    "type": "Component",
                    "id": "storiesObjectPage_stories",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "stories",
                            "navigation": {},
                            "content": {
                                "header": {
                                    "visible": true
                                }
                            },
                            "editableHeaderContent": false,
                            "controlConfiguration": {
                                "use_cases/@com.sap.vocabularies.UI.v1.LineItem#UseCases": {
                                    "tableSettings": {
                                        "enableExport": false,
                                        "enableFullScreen": false,
                                        "enablePaste": false,
                                        "selectAll": false,
                                        "personalization": {
                                            "column": false,
                                            "filter": false,
                                            "sort": false
                                        },
                                        "condensedTableLayout": true,
                                        "quickVariantSelection": {
                                            "hideTableTitle": false,
                                            "showCounts": false
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "controlAggregation": "endColumnPages",
                    "contextPattern": "/epic({key})/stories({key2})"
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {}
            }
        },
        "rootView": {
            "viewName": "sap.fe.templates.RootContainer.view.Fcl",
            "type": "XML",
            "async": true,
            "id": "appRootView"
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}